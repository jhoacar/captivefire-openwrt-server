{
    "type": "project",
    "license": "proprietary",
    "description": "Captivefire - GraphQL",
    "keywords": [
        "graphql",
        "openwrt",
        "captivefire",
        "wifi",
        "captiveportal"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.1",
        "symfony/http-foundation": "^6.1|^5.4",
        "vlucas/phpdotenv": "^5.4",
        "webonyx/graphql-php": "^0.13.8"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {},
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [],
        "post-update-cmd": [],
        "test": "phpunit"
    },
    "conflict": {},
    "extra": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9|^3.4",
        "phpunit/phpunit": "^9.0"
    }
}
