config uhttpd 'captivefire'
	list listen_http '0.0.0.0:8080'
	list listen_http '[::]:8080'
 	list listen_https '0.0.0.0:8443'
	list listen_https '[::]:8443'
	option redirect_https '1'
	list interpreter '.php=/usr/bin/php-cgi'
	option no_dirlists '0'
	option home '/app'
	option index_file 'public/index.php'
	option index_page 'public/index.php'
	option error_page '/public/index.php' # Must start with '/'
 	option cert '/app/captivefire.crt'
 	option key '/app/captivefire.key'

